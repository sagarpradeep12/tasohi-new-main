/****** Edited ******/
.button,
.button--primary {
  background-color: #e00000; /* Red */
  color: #ffffff;            /* White text */
  border: none;
  border-radius: 12px;        /* Rounded corners */
  padding: 10px 20px;
  font-weight: 600;
  transition: background-color 0.2s ease;
}

.button:hover,
.button--primary:hover {
  background-color: #c70000; /* Darker red on hover */
}


.footer__content-bottom-wrapper {
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap; /* good for responsive stacking on mobile */
}

.footer_social_icon {
  margin-left: auto;
}

@media screen and (max-width: 1450px) {   
	section.cosmical-renew-sec .btm-content.pos {    
		bottom: 50px;
		right: 85px;    
    }
}
@media screen and (max-width: 749px) {
  .footer__content-bottom-wrapper {
    flex-direction: column;
    align-items: center;
  }

  .footer_social_icon {
    margin: 10px 0 0;
  }
}

