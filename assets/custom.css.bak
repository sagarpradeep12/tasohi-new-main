section.simple-steps {}

section.simple-steps .top-head {
    padding-bottom: 50px;
}

section.simple-steps .top-head .box {}

section.simple-steps .box {
    border: 1px solid #F5F5F5;
    padding: 12px;
    border-radius: 12px;
    overflow: hidden;
}

section.simple-steps .box .image {
    position: relative;
}

